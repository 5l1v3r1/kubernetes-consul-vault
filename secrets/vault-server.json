{
    "disable_mlock": true,
    "listener": [
        {
            "tcp": {
                "tls_cert_file": "/etc/tls/vault.pem",
                "tls_key_file": "/etc/tls/vault-key.pem"
            }
        }
    ],
    "storage": {
        "consul": {
            "address": "consul.default.svc.cluster.local:8500",
            "path": "vault/",
            "token": "VAULT_CONSUL_TOKEN"
        }
    }
}
